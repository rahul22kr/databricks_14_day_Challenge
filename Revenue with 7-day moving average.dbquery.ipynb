   "source": [
    "%sql\n",
    "WITH daily AS (\n",
    "  SELECT\n",
    "    event_date,\n",
    "    SUM(revenue) AS rev\n",
    "  FROM ecommerce.gold.events\n",
    "  GROUP BY event_date\n",
    ")\n",
    "SELECT\n",
    "  event_date,\n",
    "  rev,\n",
    "  AVG(rev) OVER (\n",
    "    ORDER BY event_date\n",
    "    ROWS BETWEEN 6 PRECEDING AND CURRENT ROW\n",
    "  ) AS ma7\n",
    "FROM daily\n",
    "ORDER BY event_date;\n"
   ]
